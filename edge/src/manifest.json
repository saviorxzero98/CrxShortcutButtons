{
    "manifest_version": 2,
    "name": "Edge Shortcut Button",
    "version": "1.3.1",
    "description": "Edge常用功能鈕",
    "icons": {
        "128": "icon/128.png",
        "16": "icon/16.png",
        "48": "icon/48.png"
    },
    "browser_action": {
        "default_title": "Edge Shortcut Button",
        "default_icon": "icon/16.png",
        "default_popup": "popup.html"
    },
    "permissions": [
		"tabs", 
		"storage", 
		"notifications", 
		"http://*/*", 
		"https://*/*"
	],
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com 'unsafe-eval'; object-src 'self'"
}
